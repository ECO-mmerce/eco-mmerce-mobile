{
  "expo": {
    "name": "ECO-mmerce",
    "slug": "eco-mmerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-logo.png",
    "splash": {
      "image": "./assets/eco-merce.png",
      "resizeMode": "contain",
      "backgroundColor": "#d8e1e3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Mobile client app for ECO-mmerce",
    "githubUrl": "https://github.com/ECO-mmerce/eco-mmerce-mobile"
  }
}
